{
    "timeout": 2,
    "tail_timeout": 10,
    "cachefile": "/tmp/.watchlogd.cache",
    "docpre": "prod_",
    "proc_title": "watchlogd",
    "logfile": "/tmp/log_watchlog.log",
    "beanstalk": {
        "host": "beanstalk.yundun.com",
        "port" :"11300"
    },
    "watches": [
        {
            "type": "php_error",
            "queue": "log_php_fatal_error",
            "logdir": "/usr/local/php-5.6.20/log/",
            "glob": "php_admin_error.log"
        },
        {
            "type": "php_error",
            "queue": "log_php_fatal_error",
            "logdir": "/usr/local/php-5.6.20/log/",
            "glob": "fpm-php.www.error.log"
        },
        {
            "type": "ypf_error",
            "queue": "*",
            "logdir": "/tmp/",
            "glob": "log_phperror_*.log",
            "queue_match_file": {
                "queuePrefix": "log_phperror_",
                "queueSuffix": "",
                "fnameTrimPrefix": "log_phperror_",
                "fnameTrimSuffix": ".log"
            }
        },
        {
            "type": "log_queue",
            "queue": "*",
            "logdir": "/tmp/",
            "glob": "log_queue.log"
        }
    ]
}
